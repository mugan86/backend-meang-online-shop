{
  "name": "backend-meang",
  "version": "2.0.0",
  "description": "Proyecto Backend API GraphQL Tienda Online - MEAN+G",
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "dev": "nodemon \"src/app.ts\" --exec \"ts-node\" \"src/app.ts\" -e ts,graphql,json",
    "argument": "echo \"The value of --foo is '${npm_config_foo}'\"",
    "lint": "npx eslint . --ext .ts",
    "lint.fix": "npx eslint . --ext .ts --fix",
    "test": "npm run build && npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mugan86/backend-meang-online-shop.git"
  },
  "keywords": [
    "graphql",
    "meang",
    "api-graphql",
    "stripe",
    "vercel",
    "netlify",
    "zeitnow",
    "admin-sidebar"
  ],
  "author": "Anartz Mugika Ledo <mugan86@gmail.com> (anartz-mugika.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mugan86/backend-meang-online-shop/issues"
  },
  "homepage": "https://github.com/mugan86/backend-meang-online-shop#readme",
  "engines": {
    "node": "14.17.0",
    "npm": "7.20.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "mocks"
    ]
  },
  "dependencies": {
    "@wiicamp/graphql-merge-resolvers": "^2.1.2",
    "apollo-server-express": "^2.25.2",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^6.2.6",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.7",
    "mongodb": "^3.6.11",
    "ncp": "^2.0.0",
    "nodemailer": "^6.4.6",
    "slugify": "^1.6.0",
    "stripe": "^8.168.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.4",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongodb": "^3.6.6",
    "@types/node": "^14.14.25",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "easygraphql-tester": "^6.0.1",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.2",
    "ts-node": "^9.1.1"
  }
}
